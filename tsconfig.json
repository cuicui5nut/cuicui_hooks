{
  "compilerOptions": {
    "target": "ES5",
    "strict": true,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "jsx": "react",
    "importHelpers": true,
    "allowSyntheticDefaultImports": true,
    "strictNullChecks": true,
    "downlevelIteration": true,
    "sourceMap": true,
    "baseUrl": ".",
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "cuicui_hooks": ["packages/hooks/src"],
      "cuicui_hooks/*": ["packages/hooks/src/*", "*"]
    }
  },
  "exclude": ["node_modules", "lib", "es", "dist", "example"]

}
